{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es6",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"removeComments": true,
		"noEmitOnError": true,
		"strict": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"lib": ["dom"],
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"sourceMap": true
	}
}
